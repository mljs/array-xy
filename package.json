{
  "private": true,
  "name": "ml-array-xy",
  "version": "0.1.0",
  "main": "./src/index.js",
  "description": "Functional utilities for sets of points",
  "scripts": {
    "bootstrap": "lerna bootstrap --no-ci",
    "eslint": "eslint packages/**/*.js --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "npm run bootstrap",
    "publish": "git pull --rebase && npm run build-lerna && npm run test-publish && lerna publish",
    "test": "jest && npm run eslint",
    "test-only": "jest",
    "upgrade": "find packages -type f -name 'package.json' -maxdepth 2 -execdir ncu --upgrade \\;"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "cheminfo-tools": "^1.23.3",
    "eslint": "^8.0.1",
    "eslint-config-cheminfo": "^6.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.0.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-matcher-deep-close-to": "^3.0.2",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
